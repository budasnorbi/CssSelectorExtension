{
    "name": "PriceSelector",
    "version": "1.0",
	"description": "Get selected item selector and page url",
	"permissions":[
		"contextMenus",
		"activeTab"
	],
	"background":{
		"scripts":["event.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content_script.js"]
		}
	  ],
	"icons":{
		"16": "images/icon-16.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
    "manifest_version": 2
}